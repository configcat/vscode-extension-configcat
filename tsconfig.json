{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"skipLibCheck": true,
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": "src",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}